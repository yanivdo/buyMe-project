<form class="login_form" method="POST" {{action @submitLogin on="submit"}}>
    <label for="email">Email:</label>
{{input
  type='text'
  class='form-control'
  name='email'
  value=email
  placeholder='email'
}}
    <label for="psw"><b>Password</b></label>
{{input
  type='password'
  class='form-control'
  value=password
  placeholder='password'
}}
    <button type="submit" class="loginBtn">Login</button>
</form>